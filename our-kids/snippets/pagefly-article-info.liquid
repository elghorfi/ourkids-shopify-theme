{% if article.author != blank and article.title != blank %}<div class="pf-article__header"><h1 class="pf-article__title">{{ article.title }}</h1><div class="pf-article__meta">
    <span class='pf-article__author'>{{ article.author }}</span>
    <span class="pf-article__date">{{ article.published_at | time_tag: format: 'month_day_year' }}</span></div></div>{% endif %}